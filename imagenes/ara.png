function moveCarousel(video) {
    $("#youtubeCarouselVideos").find("iframe").each(function() { 
        $(this).get(0).contentWindow.postMessage('{"event":"command","func":"pauseVideo","args":""}', '*');
    });
    $("#youtubeCarouselVideos").find("span").each(function() { 
