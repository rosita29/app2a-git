    $('#youtubeCarouselIframe').fadeOut();
    $('#youtubeCarouselBackground').fadeOut();
    $('#youtubeCarouselIframe').html('');
}

function nextVideo() {
    if((actualVideo+1)<totalVideos) {
        actualVideo++;
        moveCarousel(actualVideo);
    }
}

function prevVideo() {
    if(actualVideo>0) {
        actualVideo--;
        moveCarousel(actualVideo);
    }
}

$("#youtubeCarouselVideos").on("swipeleft",function(){
