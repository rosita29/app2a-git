    duration*=50;
    left=2+(video*20);
    $('#youtubeCarouselButtonInner').animate({top: '+=1px', height: '-=2px',left: '-=2px', width: '+=4px'}, 100);
    $('#youtubeCarouselButtonInner').animate({left: left+'px'}, duration);
    $('#youtubeCarouselButtonInner').animate({top: '-=1px', height: '+=2px', left: (left+2)+'px', width: '-=4px'}, 100);
    
    margin=marginVideos-(video*widthVideo);
    $('#youtubeCarouselVideos').animate({marginLeft: margin+'px'}, duration+200);
    actualVideo=video;
}

function playVideo(video, id) {
    heightWindow = $(window).height();
    heightVideo = ($(window).width()>700) ? 360 : 180;
    marginTop=(heightWindow-heightVideo)/2;
